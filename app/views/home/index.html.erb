<div id="home">
  <div class="overflowin">
    <div align="center" class="container">
      <%= link_to submit_order_path, method: :POST do %>
      <%= image_tag('button-logo.svg', class: "order-btn") %>
        <% end %>
    </div>
  </div>
  <div align="center">
    <div class="container">
      <div class="dropdown">
        <button class="btn btn-default dropdown-toggle" type="button" id="menu1" data-toggle="dropdown">Order Profiles
        <span class="caret"></span></button>
        <ul class="dropdown-menu" role="menu" aria-labelledby="menu1">
          <% @order_profiles.each do |o| %>
          <li>
            <%= o['name'] %>
          </li>
          <li style="visibility:hidden"><%= o['id'] %>
          <% end %>
        </ul>
      </div>
    </div>
    <p id="notice"><%= notice %></p>
  </div>



</div>
